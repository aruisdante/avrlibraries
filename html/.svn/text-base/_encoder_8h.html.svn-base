<a class="anchor" id="a5770b79eb51a21ea2dd707125a4bd358"></a><!-- doxytag: member="Encoder.h::Encoder_Read" ref="a5770b79eb51a21ea2dd707125a4bd358" args="(char command, long *result)" -->
<div class="memitem">
<div class="memproto">
      <table class="memname">
        <tr>
          <td class="memname">char Encoder_Read </td>
          <td>(</td>
          <td class="paramtype">char&nbsp;</td>
          <td class="paramname"> <em>command</em>, </td>
        </tr>
        <tr>
          <td class="paramkey"></td>
          <td></td>
